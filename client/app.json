{
  "expo": {
    "name": "Face Blindness Aid",
    "slug": "faceblindness-aid",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "faceblindness-aid",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.faceblindnessaid.app"
    },
    "android": {
      "package": "com.faceblindnessaid.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "faceblindness-aid"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": false
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}